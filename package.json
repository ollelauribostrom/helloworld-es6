{
  "name": "helloworld-es6",
  "version": "1.0.7",
  "description": "Boilerplate for an ES6 node package/cli",
  "main": "dist/helloworld.js",
  "bin": {
    "helloworld-es6": "dist/bin/helloworld.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.3"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "nyc": {
    "report-dir": "test/coverage",
    "cache": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "scripts": {
    "start": "babel src -D -d dist --watch",
    "build": "babel src -D -d dist",
    "prepublish": "npm run build",
    "test": "mocha --timeout 10000 --require babel-core/register",
    "coverage": "nyc --temp-directory ./test/coverage/tmp mocha --timeout 10000 --require babel-core/register",
    "coveralls": "nyc report --temp-directory ./test/coverage/tmp mocha --timeout 10000 --require babel-core/register --reporter=text-lcov | coveralls",
    "lint": "eslint src test --ignore-pattern test/coverage*"
  },
  "author": "Olle Lauri Bostr√∂m <ollebostr@gmail.com> (https://github.com/ollelauribostrom)",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.12.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ollelauribostrom/helloworld-es6.git"
  },
  "keywords": [
    "es6",
    "npm-package",
    "boilerplate",
    "helloworld",
    "babel",
    "chai",
    "mocha",
    "nyc",
    "cli-boilerplate",
    "cli-template",
    "cli",
    "template"
  ],
  "bugs": {
    "url": "https://github.com/ollelauribostrom/helloworld-es6/issues"
  },
  "homepage": "https://github.com/ollelauribostrom/helloworld-es6#readme"
}
